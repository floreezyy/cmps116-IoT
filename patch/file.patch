+static void *gl2psRealloc(void *ptr, size_t size)
+{
+  void *orig = ptr;
+  if(!size) return NULL;
+  ptr = realloc(orig, size);
+  if(!ptr){
+    gl2psMsg(GL2PS_ERROR, "Couldn't reallocate requested memory");
+    free(orig);
+    return NULL;
+  }
+  return ptr;
+}
