-  prim->verts[0].xyz[2] = pos[2];
-  prim->culled = 0;
-  prim->offset = 0;
-  prim->ofactor = 0.0;
-  prim->ounits = 0.0;
-  prim->pattern = 0;
-  prim->factor = 0;
-  prim->width = 1;
